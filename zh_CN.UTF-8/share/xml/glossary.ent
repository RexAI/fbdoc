    <glossentry xml:id="fat-glossary">
      <glossterm>File Allocation Table (文件分配表)</glossterm>
      <acronym>FAT</acronym>
      <glossdef>
        <para/>
      </glossdef>
    </glossentry>

    <glossentry xml:id="fat16-glossary">
      <glossterm>File Allocation Table (16-bit) (16-位文件分配表)</glossterm>
      <acronym>FAT16</acronym>
      <glossdef>
