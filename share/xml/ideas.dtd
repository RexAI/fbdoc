<!-- $FreeBSD$ -->

<!ELEMENT ideas (cvs:keyword?, category)+>

<!ELEMENT title (#PCDATA)>
<!ATTLIST title
  id		ID	#IMPLIED
>

<!ELEMENT category (title, (idea)+)>
<!ATTLIST category
  id		ID	#IMPLIED
>

<!ELEMENT idea (title, desc)>
<!ATTLIST idea
  id		ID	#IMPLIED
>

<!ELEMENT desc (#PCDATA)>
<!ATTLIST desc
  id		ID	#IMPLIED
>

<!ENTITY % cvs
 PUBLIC "-//FreeBSD//DTD FreeBSD CVS Keywords//EN"
	"http://www.FreeBSD.org/XML/share/xml/cvs.dtd">
%cvs;

